{
  "name": "@ninetails-monorepo-react-ssr/react-kabocha-dev",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ninetails/monorepo-react-ssr.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ninetails/monorepo-react-ssr/issues"
  },
  "homepage": "https://github.com/ninetails/monorepo-react-ssr/tree/master/packages/react-kabocha#readme",
  "devDependencies": {
    "@ninetails-monorepo-react-ssr/babel-preset-monorepo-react-ssr": "^0.0.0",
    "@ninetails-monorepo-react-ssr/eslint-config-monorepo-react-ssr": "^0.0.0",
    "@ninetails-monorepo-react-ssr/helper-gen-dist-package": "^0.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-react-remove-properties": "^0.2.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
    "cross-env": "^5.2.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-dom": "^2.1.1",
    "jest-environment-jsdom": "^23.4.0",
    "jest-silent-reporter": "^0.1.1",
    "prop-types": "^15.6.2",
    "react-testing-library": "^5.3.0"
  },
  "dependencies": {
    "debounce": "^1.2.0",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2"
  },
  "scripts": {
    "build:babel:prod": "cross-env NODE_ENV=production $npm_execpath run build:babel -- -s",
    "build:babel:dev": "cross-env NODE_ENV=development $npm_execpath run build:babel -- --source-maps inline",
    "build:babel": "babel -d dist --ignore 'src/**/*.spec.js','src/**/*.test.js' src",
    "build:clean": "rm -rf dist/*",
    "build:copy:all": "$npm_execpath run build:copy:changelog && $npm_execpath run build:copy:readme",
    "build:copy:changelog": "cp CHANGELOG.md dist 2>/dev/null || :",
    "build:copy:readme": "cp README.md dist 2>/dev/null || :",
    "build:create:dist": "mkdir -p dist",
    "build:generate:package": "generate-dist-package --peer react --peer react-dom",
    "build": "$npm_execpath run build:clean && $npm_execpath run build:create:dist && $npm_execpath run build:copy:all && $npm_execpath run build:generate:package && $npm_execpath run build:babel:prod",
    "coverage": "$npm_execpath run test -- --coverage",
    "lint": "eslint src",
    "postinstall": "$npm_execpath run build",
    "test:jest": "jest",
    "test:watch": "$npm_execpath run test:jest -- --watch",
    "test": "$npm_execpath run test:jest -- --reporters jest-silent-reporter",
    "watch": "$npm_execpath run build:clean && $npm_execpath run build:create:dist && $npm_execpath run build:copy:all && $npm_execpath run build:generate:package && $npm_execpath run build:babel:dev -- -w"
  }
}
